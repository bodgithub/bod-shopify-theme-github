<a href="{{ url }}" 
   title="{{ title | escape }}" 
   role="link" 
   class="text-button {{ class }} text-button--has-icon" 
   {% if block %}{{ block.shopify_attributes }}{% endif %}
   {% if open_side_panel %}data-open-side-panel="true"{% endif %}>
   {{ title }} 
   <span>{% render 'svg-icons' with 'thb-caret-right' %}</span>
</a>

