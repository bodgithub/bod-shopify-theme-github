    "multicolumn_KdNre6": {
      "type": "multicolumn",
      "blocks": {
        "column_9tKhGi": {
          "type": "column",
          "settings": {
            "image_full_width": false,
            "image_link": "",
            "title": "",
            "text": "<h5><strong>1. Supports insulin signaling</strong></h5><h4><em>Your metabolic foundation</em></h4><p>Inositol plays a role in insulin signaling, helping the body respond more effectively to insulin.* This supports steadier blood sugar levels and reduces the metabolic stress that can disrupt hormones.</p>",
            "link_label": "",
            "link": "",
            "eli_desktop_cols": "45% 18% 18% 18%",
