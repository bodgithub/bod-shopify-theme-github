        },
        "column_ppertg": {
          "type": "column",
          "disabled": true,
          "settings": {
            "image_full_width": false,
            "image_link": "",
            "title": "",
            "text": "<h5><strong>Clean Formula, Nothing Extra</strong></h5><h4><em>Plays well with your stack</em></h4><p>No fillers, no sweeteners, no additives that interfere with absorption or other supplements.</p>",
            "link_label": "",
            "link": "",
            "eli_desktop_cols": "45% 18% 18% 18%",
